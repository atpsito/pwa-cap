{
  "name": "next_boilerplate",
  "version": "0.62.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "npm start",
    "test": "jest --watchAll",
    "test:all": "npm run test -- --watchAll=false --coverage",
    "test:ci": "cross-env CI=true jest --runInBand",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start:build http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern !cypress/.eslintrc.js --ext .js,jsx,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "build": "next build",
    "validate": "cross-env CI=true npm-run-all --parallel test test:e2e check-types check-format lint build",
    "validate:ci": "npm-run-all --parallel check-types check-format lint",
    "deploy": "echo todo"
  },
  "dependencies": {
    "@bugsnag/js": "^7.1.1",
    "@bugsnag/plugin-react": "^7.1.1",
    "axios": "^0.19.2",
    "env-cmd": "^10.1.0",
    "eslint-config-universe": "^3.0.1",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.2.0",
    "i18next-xhr-backend": "^3.2.2",
    "next": "^10.0.0",
    "polished": "^3.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-error-boundary": "^2.2.1",
    "react-i18next": "^11.5.0",
    "react-query": "^1.5.3",
    "react-query-devtools": "^1.1.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.4.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/jest-in-case": "^1.0.2",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-query": "^1.1.2",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "avn": "^0.2.4",
    "avn-nvm": "^0.2.2",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "cypress": "^4.6.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-flowtype": "^5.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.3",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.4.0",
    "jest-in-case": "^1.0.2",
    "jest-styled-components": "^7.0.2",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "start-server-and-test": "^1.11.0",
    "test-data-bot": "^0.8.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
